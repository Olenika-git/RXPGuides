<Ui xmlns="http://www.blizzard.com/wow/ui/">
    <Button name="RXP_IU_AH_ShoppingList_ItemRow" hidden="false" virtual="true" enableMouse="true">
        <Size x="256" y="17"/>
        <Layers>
            <Layer level="BACKGROUND">
                <FontString parentKey="Name" inherits="GameFontNormal" justifyH="LEFT" text="$parentName">
                    <Anchors>
                        <Anchor point="LEFT">
                            <Offset>
                                <AbsDimension x="20" y="0"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                </FontString>
                <Texture file="Interface\AuctionFrame\UI-AuctionItemNameFrame">
                    <Size x="10" y="16"/>
                    <Anchors>
                        <Anchor point="LEFT" relativeKey="$parent.left" relativePoint="RIGHT">
                            <Offset>
                                <AbsDimension x="-4" y="0"/>
                            </Offset>
                        </Anchor>
                        <Anchor point="RIGHT" relativeKey="$parent.right" relativePoint="LEFT">
                            <Offset>
                                <AbsDimension x="16" y="0"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                    <TexCoords left="0.078125" right="0.75" top="0" bottom="1.0"/>
                </Texture>
            </Layer>
            <Layer level="OVERLAY">
                <FontString parentKey="Status" inherits="GameFontHighlightSmall" text="(11/60)">
                    <Anchors>
                        <Anchor point="RIGHT" relativeKey="$parent" relativePoint="RIGHT">
                            <Offset>
                                <AbsDimension x="-4" y="0"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Frames>
            <Button parentKey="ItemFrame">
                <Size x="16" y="16"/>
                <Anchors>
                    <Anchor point="LEFT"/>
                </Anchors>
                <Layers>
                    <Layer level="BORDER">
                        <Texture parentKey="IconTexture"/>
                    </Layer>
                    <Layer level="OVERLAY">
                        <Texture parentKey="IconBorder" file="Interface\Common\WhiteIconFrame" hidden="true">
                            <Size x="16" y="16"/>
                            <Anchors>
                                <Anchor point="CENTER"/>
                            </Anchors>
                        </Texture>
                    </Layer>
                    <Layer level="OVERLAY" textureSubLevel="1">
                        <Texture parentKey="IconOverlay" hidden="true">
                            <Size x="16" y="16"/>
                            <Anchors>
                                <Anchor point="CENTER"/>
                            </Anchors>
                        </Texture>
                    </Layer>
                </Layers>
                <Scripts>
                    <OnEnter>
                        local p = self:GetParent()
                        if p.itemLink then
                            GameTooltip:SetOwner(self);
                            GameTooltip:SetHyperlink(p.itemLink);
                            GameTooltip:Show();
                        end
                        RXP.auctionHouse.shoppingList.RowOnEnter(p)
                    </OnEnter>
                    <OnLeave>
                        RXP.auctionHouse.shoppingList.RowOnLeave(self:GetParent())
						GameTooltip_Hide();
						ResetCursor();
                    </OnLeave>
                    <OnClick>
                        RXP.auctionHouse.shoppingList.RowOnClick(self:GetParent())
                    </OnClick>
                </Scripts>
                <NormalTexture parentKey="NormalTexture" file="Interface\Buttons\UI-Quickslot2">
                    <Size x="16" y="16"/>
                    <Anchors>
                        <Anchor point="CENTER"/>
                    </Anchors>
                </NormalTexture>
                <HighlightTexture file="Interface\Buttons\ButtonHilight-Square" alphaMode="ADD"/>
            </Button>
        </Frames>
        <Scripts>
            <OnEnter>
                RXP.auctionHouse.shoppingList.RowOnEnter(self)
            </OnEnter>
            <OnLeave>
                RXP.auctionHouse.shoppingList.RowOnLeave(self)
            </OnLeave>
            <OnClick>
                RXP.auctionHouse.shoppingList.RowOnClick(self)
            </OnClick>
        </Scripts>
        <HighlightTexture parentKey="Highlight" file="Interface\HelpFrame\HelpFrameButton-Highlight" alphaMode="ADD">
            <Anchors>
                <Anchor point="TOPLEFT">
                    <Offset>
                        <AbsDimension x="0" y="-1"/>
                    </Offset>
                </Anchor>
                <Anchor point="BOTTOMRIGHT">
                    <Offset>
                        <AbsDimension x="-4" y="1"/>
                    </Offset>
                </Anchor>
            </Anchors>
            <TexCoords left="0" right="1.0" top="0" bottom="0.578125"/>
        </HighlightTexture>
    </Button>
    <Frame name="RXP_IU_AH_ShoppingList_SideBar" toplevel="true" parent="UIParent" enableMouse="true" hidden="true">
        <Size>
            <AbsDimension x="187" y="389"/>
        </Size>
        <Layers>
            <Layer level="BACKGROUND">
                <Texture name="$parentTop" file="Interface\AuctionFrame\AuctionHouseDressUpFrame-Top">
                    <Size>
                        <AbsDimension x="256" y="256"/>
                    </Size>
                    <Anchors>
                        <Anchor point="TOPLEFT"/>
                    </Anchors>
                </Texture>
                <FontString name="RXP_IU_AH_ShoppingList_Title" inherits="GameFontNormal" text="Shopping List">
                    <Anchors>
                        <Anchor point="TOP" relativeTo="RXP_IU_AH_ShoppingList_SideBar" relativePoint="TOP">
                            <Offset>
                                <AbsDimension x="0" y="-14"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                </FontString>
                <Texture file="Interface\AuctionFrame\AuctionHouseDressUpFrame-Bottom">
                    <Size>
                        <AbsDimension x="256" y="256"/>
                    </Size>
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="$parentTop" relativePoint="BOTTOMLEFT"/>
                    </Anchors>
                </Texture>
            </Layer>
            <Layer level="ARTWORK">
                <Texture parentKey="BGTopLeft" name="$parentBackgroundTop">
                    <Size>
                        <AbsDimension x="171" y="282"/>
                    </Size>
                    <Anchors>
                        <Anchor point="TOPLEFT">
                            <Offset>
                                <AbsDimension x="5" y="-14"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                    <TexCoords left="0" right="0.61" top="0" bottom="1.0"/>
                </Texture>
                <Texture parentKey="BGBottomLeft" name="$parentBackgroundBot">
                    <Size>
                        <AbsDimension x="171" y="83"/>
                    </Size>
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="$parentBackgroundTop" relativePoint="BOTTOMLEFT"/>
                    </Anchors>
                    <TexCoords left="0" right="0.61" top="0" bottom="0.588"/>
                </Texture>
            </Layer>
        </Layers>
        <Frames>
            <Button parentKey="ImportButton" name="$parentImportButton" inherits="UIPanelButtonTemplate" text="RESET">
                <Size>
                    <AbsDimension x="80" y="22"/>
                </Size>
                <Anchors>
                    <Anchor point="TOP" relativeTo="RXP_IU_AH_ShoppingList_Title" relativePoint="BOTTOM">
                        <Offset>
                            <AbsDimension x="0" y="-2"/>
                        </Offset>
                    </Anchor>
                </Anchors>
                <Scripts>
                    <OnClick>
                        print(".ImportButton:Onclick")
                    </OnClick>
                </Scripts>
            </Button>
            <Frame name="$parentScrollFrame" inherits="WowScrollBoxList" parentKey="ScrollBox">
                <Anchors>
                    <Anchor point="TOPLEFT">
                        <Offset>
                            <AbsDimension x="5" y="-52"/>
                        </Offset>
                    </Anchor>
                    <Anchor point="BOTTOMRIGHT">
                        <Offset>
                            <AbsDimension x="-10" y="22"/>
                        </Offset>
                    </Anchor>
                </Anchors>
                <Frames>
                    <!-- Unused/hidden, but needed for easy display -->
                    <EventFrame name="RXP_IU_AH_ShoppingList_ScrollBar" inherits="WowTrimScrollBar" parentKey="ScrollBar" hidden="true">
                        <Anchors>
                            <Anchor point="TOPRIGHT" relativeTo="$parent" relativePoint="TOPRIGHT"/>
                            <Anchor point="BOTTOMRIGHT" relativeTo="$parent" relativePoint="BOTTOMRIGHT"/>
                        </Anchors>
                    </EventFrame>
                </Frames>
            </Frame>
            <Button name="$parentCloseButton" inherits="UIPanelCloseButton">
                <Anchors>
                    <Anchor point="CENTER" relativeTo="RXP_IU_AH_ShoppingList_SideBar" relativePoint="TOPRIGHT">
                        <Offset>
                            <AbsDimension x="-15" y="-16"/>
                        </Offset>
                    </Anchor>
                </Anchors>
                <Layers>
                    <Layer level="BACKGROUND">
                        <Texture file="Interface\AuctionFrame\AuctionHouseDressUpFrame-Corner">
                            <Size>
                                <AbsDimension x="32" y="32"/>
                            </Size>
                            <Anchors>
                                <Anchor point="TOPRIGHT" relativeTo="RXP_IU_AH_ShoppingList_SideBar">
                                    <Offset>
                                        <AbsDimension x="-5" y="-5"/>
                                    </Offset>
                                </Anchor>
                            </Anchors>
                        </Texture>
                    </Layer>
                </Layers>
                <Scripts>
                    <OnClick>
                        HideUIPanel(self:GetParent());
                    </OnClick>
                </Scripts>
            </Button>
        </Frames>
    </Frame>
</Ui>
